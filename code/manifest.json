{
	"manifest_version": 2,
	"name": "Gencon Calendar Link",
	"version": "1.1",

	"description": "Adds a Google Calendar link to gencon event pages",
	"icons": {
		"48": "icons/calendar_icon_g.svg",
		"96": "icons/calendar_icon_g.svg"
	},

	"content_scripts":[
		{
			"matches": ["*://*.gencon.com/events/*"],
			"js": ["gencon_calendar.js"]
		}
	]
}